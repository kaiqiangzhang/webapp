<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.saygood.soft.model">
    <class name="UserInfo" table="user_info">
        <id name="id" column="id" type="java.lang.Integer">
            <generator class="increment"/>
        </id>
        <property name="account" column="account" type="java.lang.String" />
        <property name="pwd" column="pwd" type="java.lang.String" />
        <property name="img" column="img" type="java.lang.String" />
        <property name="token" column="token" type="java.lang.String" />
        <property name="passport" column="passport" type="java.lang.String" />
        <property name="email" column="email" type="java.lang.String" />
        <property name="nickname" column="nickname" type="java.lang.String" />
        <property name="note" column="note" type="java.lang.String" />
        <property name="medal" column="medal" type="java.lang.Float" />
        <property name="points" column="points" type="java.lang.Float" />
        <property name="visit_counts" column="visit_counts" type="java.lang.Float" />
        <property name="fans" column="fans" type="java.lang.Float" />
        <property name="fan_to" column="fan_to" type="java.lang.Float" />
        <property name="zhaunfa" column="zhaunfa" type="java.lang.Float" />
        <property name="shoucang" column="shoucang" type="java.lang.Float" />
    </class>
</hibernate-mapping>